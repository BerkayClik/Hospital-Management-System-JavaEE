<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setApp</servlet-name>
        <servlet-class>MakeAppServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setApp</servlet-name>
        <url-pattern>/patient/makeAppointment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>makeApp</servlet-name>
        <servlet-class>SetAppointmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>makeApp</servlet-name>
        <url-pattern>/patient/setAppointment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>makeApp2</servlet-name>
        <servlet-class>MakeAppServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>makeApp2</servlet-name>
        <url-pattern>/patient/makeAppointment2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setOffDays</servlet-name>
        <servlet-class>SetOffDayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setOffDays</servlet-name>
        <url-pattern>/doctor/setOD</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setDeptName</servlet-name>
        <servlet-class>SetDeptNamesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setDeptName</servlet-name>
        <url-pattern>/setDeptNames</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setDocName</servlet-name>
        <servlet-class>SetDoctorNamesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setDocName</servlet-name>
        <url-pattern>/patient/setDoctorNames</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setDocName2</servlet-name>
        <servlet-class>SetDoctorNames2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setDocName2</servlet-name>
        <url-pattern>/patient/setDoctorNames2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getRoomNames</servlet-name>
        <servlet-class>SetRoomNamesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getRoomNames</servlet-name>
        <url-pattern>/setRoomNames</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showAvailDoctor</servlet-name>
        <servlet-class>ShowRoomAvailability2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAvailDoctor</servlet-name>
        <url-pattern>/doctor/showRoomAvailability2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showAvailNurse</servlet-name>
        <servlet-class>showRoomAvailability</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAvailNurse</servlet-name>
        <url-pattern>/nurse/showRoomAvailability</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showOD</servlet-name>
        <servlet-class>ShowOffDaysServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showOD</servlet-name>
        <url-pattern>/doctor/showOD</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReserveRoom</servlet-name>
        <servlet-class>ReserveRoomServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReserveRoom</servlet-name>
        <url-pattern>/doctor/reserveRoom</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>upcomingAppPatient</servlet-name>
        <servlet-class>UpcomingAppointmentsServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upcomingAppPatient</servlet-name>
        <url-pattern>/patient/upcomingApp2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>upcomingAppDoctor</servlet-name>
        <servlet-class>UpcomingAppointmentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upcomingAppDoctor</servlet-name>
        <url-pattern>/doctor/upcomingApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>previousAppPatient</servlet-name>
        <servlet-class>PreviousAppointmentsServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>previousAppPatient</servlet-name>
        <url-pattern>/patient/previousApp2</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>previousAppDoctor</servlet-name>
    <servlet-class>PreviousAppointmentsServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>previousAppDoctor</servlet-name>
        <url-pattern>/doctor/previousApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>statistics</servlet-name>
        <servlet-class>AdminStatisticsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>statistics</servlet-name>
        <url-pattern>/admin/statistics</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userStats</servlet-name>
        <servlet-class>UserStatsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userStats</servlet-name>
        <url-pattern>/admin/userStats</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>filterAppointments</servlet-name>
        <servlet-class>FilterAppServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>filterAppointments</servlet-name>
        <url-pattern>/patient/filterApp</url-pattern>
    </servlet-mapping>
</web-app>
